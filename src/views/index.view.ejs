  <!-- Questions & Answers SECTION -->
  <div class="questions_and_answers_section">
    <div class="container">
      <div class="section_heading">
        <h3>Questions & Answers</h3>
        <div class="icon_and_subheading">
          <img src="../images/chat.png" alt="">
          <h5><span class="bold">10 million+</span> questions answered by doctors</h5>
        </div>
      </div>


      <div class="questions">

        <!-- question -->
        <% questions.forEach((question) => { %>
        <div class="question">
          <div class="top_heading_profile_and_img_section">
            <div class="heading_and_profile">
              <h2><%= question.question %></h2>
              <div class="profile_section">
                <div class="profile_picture">
                  <img src="<%= question.doctorId.image %>" alt="">
                </div>

                <div class="name_and_speciality">
                  <h3 class="name"><%= question.doctorId.name %></h3>
                  <p class="speciality"><%= question.doctorId.category %>, <%= question.doctorId.city %></p>
                </div>
              </div>
            </div>

            <div class="question_image_container">
              <img src="<%= question.img %>" alt="">
            </div>
          </div>

          <div class="description">
            <p><%= question.answer %></p>
          </div>

          <div class="wishlist_and_save">
            <img src="../images/wishlist.png" alt="">
            <img src="../images/saved.png" alt="">
          </div>
        </div>
        <% }) %>

        <!-- Ask a question section -->
        <div class="ask_a_question_section">
          <!-- Heading section -->
          <div class="heading_section">
            <img
              src="https://assets.lybrate.com/q_auto,f_auto,h_28,w_28,c_fill/imgs/product/Homepage/ic_ask_question_color_48x48.png"
              alt="">

            <div class="heading_content">
              <h3>Ask a Question</h3>
              <p>Get FREE multiple opinions from Doctors</p>
            </div>
          </div>

          <!-- Input section -->
          <textarea class="ask_question_input" type="text"
            placeholder="Enter question for you or someone else. E.g. my left eye is red and swollen"></textarea>

          <!-- Submit section -->
          <div class="submit_section">
            <p>Submit Anonymously</p>
            <button class="btn submit_button">Sumbit</button>
          </div>
        </div>

        <div class="more_questions_answers">
          <p>View More Questions & Answers</p>
        </div>

      </div> <!-- Questions section end -->


    </div>
  </div> <!-- Questions and answers section end -->

