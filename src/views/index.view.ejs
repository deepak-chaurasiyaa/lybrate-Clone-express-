          </div>
        </div>
        <% } %>


        <div class="sidebar_tips_section">

          <div class="sidebar_tips">

            <% for(let i = 1; i < tips.length; i++) { %>
            <div class="tip">
              <div class="img_container">
                <img src="<%= tips[i].doctorId.image %>" alt="">
              </div>

              <div class="title_and_name">
                <h3><%= tips[i].title %></h3>
                <p><%= tips[i].doctorId.name %>, <%= tips[i].doctorId.category %></p>
              </div>
            </div>
            <% } %>

          </div> <!-- Sidebar tips ends -->

          <div class="view_more_button">
            <button>View More Health Tips</button>
          </div>

        </div> <!-- Sidebar tips section ends -->

